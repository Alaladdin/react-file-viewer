{
  "name": "react-file-viewer",
  "version": "1.2.4-fix-photo-viewer.2",
  "description": "Extendable file viewer for web",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "engines": {
    "node": "^14.17.6",
    "npm": "^8.19.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plangrid/react-file-viewer.git"
  },
  "scripts": {
    "dev": "webpack -d --watch",
    "build": "webpack --progress",
    "start": "node ./scripts/start.js",
    "lint": "node_modules/.bin/eslint ./src/**/*.jsx",
    "test": "node_modules/.bin/jest --env=jsdom --watch",
    "jest": "node_modules/.bin/jest --env=jsdom",
    "tag-and-publish": "node ./scripts/publish.js"
  },
  "author": "PlanGrid <opensource@plangrid.com>",
  "contributors": [
    {
      "name": "Alexei Schiopu",
      "email": "alexxschiopu@gmail.com",
      "url": "http://schiopu.me"
    },
    {
      "name": "Aaron Norby",
      "email": "aaron.norby@gmail.com"
    },
    {
      "name": "Viktoriya Savkina",
      "email": "viktoriya.savkina@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plangrid/react-file-viewer/issues"
  },
  "homepage": "https://github.com/plangrid/react-file-viewer#readme",
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "autoprefixer": "^10.4.19",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^8",
    "babel-runtime": "^6.20.0",
    "chalk": "^5.3.0",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "inquirer": "^10.1.6",
    "jest": "^29.7.0",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "comma-separated-values": "^3.6.4",
    "mammoth": "1.8.0",
    "pdfjs-dist": "1.9.607",
    "prop-types": "^15.8.1",
    "react-data-grid": "^7.0.0-beta.44",
    "react-visibility-sensor": "^5.1.1",
    "three": "0.167.1",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|scripts|example_files)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupTests.js"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "webpack": "webpack.config.js"
      }
    }
  }
}
